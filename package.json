{
  "name": "finance-prototype",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tauri": "tauri",
    "cargo": "cd src-tauri; cargo",
    "test:backend": "cd src-tauri; cargo tarpaulin -o html --output-dir target/coverage --skip-clean",
    "test:frontend": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test": "concurrently --kill-others \"npm run test:backend\" \"npm run test:frontend\"",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tauri-apps/cli": "^1.5.2",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/svelte": "^4.0.4",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^0.34.6",
    "concurrently": "^8.2.2",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.69.3",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.5.11",
    "vitest": "^0.34.6"
  },
  "type": "module",
  "dependencies": {
    "@tauri-apps/api": "^1.5.0"
  }
}
